@model WebApplication6.Models.Kategori
@{
    ViewBag.Title = "KategoriEkle";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h2>Kategori Ekle</h2>
<form action="" class="form form-group" method="post">
    <label style="margin-top:15px">Duyuru Kategori Adı Giriniz</label>
    @Html.TextBoxFor(x => x.KategoriAd, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>
