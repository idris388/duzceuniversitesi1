@using WebApplication6.Models
@model List<FakülteGenelBilgi>
@{
    ViewBag.Title = "LisansÜstüÖgrenci";
    Layout = "~/Views/Shared/_Tema.cshtml";
}

<section class="breadcrumbs">
    <div class="container">

        <div class="d-flex justify-content-between align-items-center">
            <h2>
                Lisansüstü Eğitim Enstitüsü
            </h2>
            <ol>
                <li><a href="/Anasayfa/Index">Anasayfa</a></li>
                <li><a href="/Fakülteler/LisansÜstüGenelBilgi">Genel Bilgi</a></li>
                <li><a href="/Fakülteler/LisansÜstüPersonel">Personel</a></li>
                <li><a href="/Fakülteler/LisansÜstüYonetim">Yönetim</a></li>
                <li><a href="/Fakülteler/LisansÜstüÖgrenci">Öğrenci</a></li>
                <li><a href="/Fakülteler/LisansÜstüFakülte">Dergiler</a></li>
                <li>
                    Lisansüstü Eğitim Enstitüsü
                </li>
            </ol>
        </div>

    </div>
</section>
@foreach (var item in Model)
{

    <section id="blog" class="blog">
        <div class="container" data-aos="fade-up">
            <div class="row">
                <div class="col-lg-8 entries">
                    <article class="entry entry-single">
                        <div class="entry-content">
                            <p class="ckeditor">
                                @Html.Raw(item.Ogrenci)
                            </p>
                        </div>
                    </article><!-- End blog entry -->
                </div><!-- End blog entries list -->
            </div>

        </div>
    </section>
}
<script src="~/ckeditor/ckeditor.js"></script>